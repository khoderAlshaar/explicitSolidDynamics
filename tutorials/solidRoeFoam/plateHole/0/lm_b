FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      lm_b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    right
    {
        type    		tractionLinearMomentum;
        loadingType		traction;
        traction		(10000 0 0);
        value 			uniform (0 0 0);
    }
  
    hole
     {
        type    tractionLinearMomentum;
        loadingType		traction;
        traction		(0 0 0);
        value 		uniform (0 0 0);
    }  
    sides
    {
        type            tractionLinearMomentum;
        loadingType		traction;
        traction		(0 0 0);
        value 	    	uniform (0 0 0);
    }
    top
    {
        type            tractionTraction;
        loadingType		traction;
        traction		(0 0 0);
        value 	    	uniform (0 0 0);
    }
    
    symmetric
    {
        type            symmetricLinearMomentum;
        value 		    uniform (0 0 0);
    }
   


}

