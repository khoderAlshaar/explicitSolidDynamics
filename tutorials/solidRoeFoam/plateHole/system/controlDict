FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     solidRoeFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.005;

timeStepping    variable;

cfl             0.5;

deltaT          0;

writeControl    runTime;

writeInterval   0.001;

purgeWrite      5;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable on;


