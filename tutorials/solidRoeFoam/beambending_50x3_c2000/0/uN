FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      uN;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            calculated;
    }
    left
    {
        type            calculated;
    }
    right
    {
        type            calculated;
    }
    bottom
    {
        type            calculated;
    }
    sides
    {
        type            calculated;
    }
}


